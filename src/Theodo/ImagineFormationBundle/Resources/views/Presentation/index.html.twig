{% extends 'TheodoImagineFormationBundle:layout:layout.html.twig' %}

{% block content %}
    {% include 'TheodoImagineFormationBundle:Presentation:presentation.html.twig' %}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
        '@TheodoImagineFormationBundle/Resources/public/less/presentation.less' %}
        <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('bundles/theodoimagineformation/highlightjs/highlight.pack.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/theodoimagineformation/impress.js/js/impress.js') }}" type="text/javascript"></script>

    <script>impress().init();</script>
    <script>hljs.initHighlightingOnLoad();</script>
{% endblock %}
