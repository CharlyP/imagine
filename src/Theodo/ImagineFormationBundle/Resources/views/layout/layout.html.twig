{% extends '::base.html.twig' %}

{% block body %}
    <div id="header">
        {% block header %}
            {% include 'TheodoImagineFormationBundle:layout:header.html.twig' %}
        {% endblock %}
    </div>

    <div id="content">
        {% block content %}{% endblock %}
    </div>
{% endblock %}

{% block stylesheets %}
    <link href="{{ asset('bundles/theodoimagineformation/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('bundles/theodoimagineformation/highlightjs/styles/zenburn.css') }}" rel="stylesheet" />
    {% stylesheets
        '@TheodoImagineFormationBundle/Resources/public/less/main.less' %}
        <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
{% endblock %}
